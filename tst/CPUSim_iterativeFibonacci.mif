26        :  01110100;              -- clear A4
27        :  00000000;              -- 
28        :  00100000;              -- subtract A0 A6
29        :  11000000;              -- 
2A        :  00100011;              -- subtract A3 A0
2B        :  00000000;              -- 
2C        :  01000011;              -- jmpz A3_finally
2D        :  00110010;              -- 
2E        :  00001011;              -- loadi A3_two
[2F..30]:  00111000;              -- jump _fibonacci_n
31        :  00011110;              -- 
32        :  00010001;              -- _finally: storei A1 IO
33        :  11111110;              -- 
34        :  10100000;              -- return
[35..36]:  00000000;              -- _one: .data 2 1
37        :  00000001;              -- 
38        :  00000000;              -- _two: .data 2 2
39        :  00000010;              -- 
[3A..FF]:  00000000;              -- 
END;
