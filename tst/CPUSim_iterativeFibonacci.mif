17        :  00011110;              -- 
18        :  00111000;              -- jump _eop
19        :  00011100;              -- 
1A        :  00010001;              -- _print_fib1: storei A1 IO
1B        :  11111110;              -- 
[1C..1D]:  00000000;              -- 
