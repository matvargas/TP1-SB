03        :  00110110;              -- 
04        :  00001010;              -- loadi A2_one
05        :  00110110;              -- 
06        :  00001011;              -- loadi A3_two
07        :  00111000;              -- 
08        :  00001110;              -- loadi A6_one
09        :  00110110;              -- 
0A        :  00001111;              -- _fib_1: loadi A7_one
0B        :  00110110;              -- 
0C        :  00100111;              -- subtract A7 A0
0D        :  00000000;              -- 
0E        :  01000111;              -- jmpz A7_print_fib1
0F        :  00011010;              -- 
